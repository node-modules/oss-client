{
  "name": "oss-client",
  "version": "2.5.0",
  "engines": {
    "node": ">= 16.0.0"
  },
  "description": "Aliyun OSS(Object Storage Service) Node.js Client",
  "scripts": {
    "lint": "oxlint",
    "pretest": "npm run lint -- --fix",
    "test": "vitest run --test-timeout 15000",
    "cov": "npm run test -- --coverage",
    "preci": "npm run lint",
    "ci": "npm run cov && npm run prepublishOnly && attw --pack",
    "prepublishOnly": "tshy && tshy-after",
    "prepare": "husky"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown --cache",
    "*.{ts,js,json,md,yml}": [
      "prettier --ignore-unknown --write",
      "oxlint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:node-modules/oss-client.git"
  },
  "keywords": [
    "oss",
    "client",
    "file",
    "aliyun"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/node-modules/oss-client/issues"
  },
  "homepage": "https://github.com/node-modules/oss-client",
  "dependencies": {
    "is-type-of": "^2.0.1",
    "mime": "^3.0.0",
    "ms": "^2.1.3",
    "oss-interface": "^1.3.0",
    "stream-wormhole": "^2.0.0",
    "urllib": "^4.6.2",
    "utility": "^2.1.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@eggjs/oxlint-config": "^1.0.0",
    "@eggjs/tsconfig": "^1.1.0",
    "@types/mime": "^3.0.1",
    "@types/ms": "^0.7.31",
    "@types/node": "^20.3.1",
    "@types/xml2js": "^0.4.12",
    "@vitest/coverage-v8": "^3.1.3",
    "husky": "^9.1.7",
    "oxlint": "^0.16.10",
    "prettier": "^3.5.3",
    "read-env-value": "^1.0.1",
    "tshy": "^1.0.0",
    "tshy-after": "^1.0.0",
    "typescript": "^5.2.2",
    "vitest": "^3.1.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "source": "./src/index.ts",
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "source": "./src/index.ts",
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "types": "./dist/commonjs/index.d.ts",
  "main": "./dist/commonjs/index.js"
}
