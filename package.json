{
  "name": "oss-client",
  "version": "1.0.0",
  "description": "Aliyun OSS(Object Storage Service) Node.js Client",
  "main": "lib/client.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "npm run tsc && mocha -t 120000 -r should -r dotenv/config test/node/*.test.js test/node/**/*.test.js",
    "test-cov": "npm run tsc && nyc --reporter=lcov node_modules/.bin/_mocha -t 120000 -r should test/node/*.test.js test/node/**/*.test.js",
    "detect-secrets": "node task/detect-secrets",
    "tsc": "npm run tsc:clean && npm run tsc:build",
    "tsc:build": "tsc -b tsconfig.json tsconfig-cjs.json",
    "tsc:watch": "tsc -b tsconfig.json tsconfig-cjs.json --watch",
    "tsc:clean": "tsc -b tsconfig.json tsconfig-cjs.json --clean "
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:node-modules/oss-client.git"
  },
  "keywords": [
    "oss",
    "client",
    "file",
    "aliyun"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/node-modules/oss-client/issues"
  },
  "engines": {
    "node": ">= 14.17.0"
  },
  "ci": {
    "version": "14, 16, 18"
  },
  "homepage": "https://github.com/node-modules/oss-client",
  "devDependencies": {
    "egg-ci": "^2.1.0",
    "eslint": "^8.25.0",
    "eslint-config-egg": "^12.1.0",
    "mm": "^2.0.0",
    "should": "^11.0.0",
    "sinon": "^1.17.7",
    "stream-equal": "^1.1.0",
    "timemachine": "^0.3.0"
  },
  "dependencies": {
    "address": "^1.2.0",
    "agentkeepalive": "^3.4.1",
    "copy-to": "^2.0.1",
    "dateformat": "^2.0.0",
    "destroy": "^1.0.4",
    "end-or-error": "^1.0.1",
    "get-ready": "^1.0.0",
    "humanize-ms": "^1.2.0",
    "is-type-of": "^1.0.0",
    "js-base64": "^2.5.2",
    "jstoxml": "^2.0.0",
    "merge-descriptors": "^1.0.1",
    "mime": "^2.4.5",
    "platform": "^1.3.1",
    "pump": "^3.0.0",
    "sdk-base": "^2.0.1",
    "stream-http": "2.8.2",
    "stream-wormhole": "^1.0.4",
    "urllib": "^2.33.1",
    "utility": "^1.8.0",
    "xml2js": "^0.4.16"
  }
}
